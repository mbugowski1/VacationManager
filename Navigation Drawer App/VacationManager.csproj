    <None Remove="Assets\closeBtn_default.png" />
    <None Remove="Assets\closeBtn_MouseOver.png" />
    <None Remove="Assets\holiday_1.png" />
    <None Remove="Assets\holiday_2.png" />
    <None Remove="Assets\holiday_3.png" />
    <None Remove="Assets\holiday_4.png" />
    <None Remove="Assets\holiday_5.png" />
    <None Remove="Assets\holiday_6.png" />
    <None Remove="Assets\holiday_7.png" />
    <None Remove="Assets\TgBtn_default.png" />
    <None Remove="Assets\tgBtn_MouseOver.png" />
    <None Remove="logo.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Background.jpg" />
    <Resource Include="Assets\closeBtn_default.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MaterialDesignThemes" Version="4.1.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\VacationManagerBackend\VacationManagerBackend.csproj" />
